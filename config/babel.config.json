{
  "presets": [
    [
      "minify",
      {
        "builtIns": false,
        "evaluate": false,
        "mangle": false
      }
    ],
    "@babel/preset-modules"
  ],
  "plugins": [
    [
      "template-html-minifier",
      {
        "modules": {
          "lit-html": [
            "html"
          ],
          "lit-element": [
            "html",
            {
              "name": "css",
              "encapsulation": "style"
            }
          ]
        },
        "strictCSS": true,
        "htmlMinifier": {
          "collapseWhitespace": true,
          "conservativeCollapse": true,
          "removeComments": true,
          "caseSensitive": true
        }
      }
    ]
  ]
}
